{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.vue",
    "src/**/**/*.vue"
  ],
  "forceConsistentCasingInFileNames": true,
  "compilerOptions": {
    "types": [
      "@intlify/unplugin-vue-i18n/messages"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
